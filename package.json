{
	"name": "adabuild",
	"displayName": "adabuild",
	"description": "Custom extension for Adaptiva front-end Angular Build System",
	"version": "0.0.1-rc1",
	"author": "Jesse Rogers <jesse.rogers@adaptiva.com>",
	"publisher": "Adaptiva",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"contributes": {
		"commands": [
			{
				"command": "adabuild.build",
				"title": "adabuild: Build Project"
			},
			{
				"command": "adabuild.incrementalbuild",
				"title": "adabuild: Build Project (Incremental)"
			},
			{
				"command": "adabuild.buildall",
				"title": "adabuild: Build All Projects"
			},
			{
				"command": "adabuild.copytsconfigdev",
				"title": "adabuild: Set TS Config (DEV)"
			},
			{
				"command": "adabuild.copytsconfigprod",
				"title": "adabuild: Set TS Config (PROD)"
			},
			{
				"command": "adabuild.debugapplication",
				"title": "adabuild: Debug Application"
			},
			{
				"command": "adabuild.buildserverjar",
				"title": "adabuild: Build Server Jar"
			},
			{
				"command": "adabuild.buildclientjar",
				"title": "adabuild: Build Client Jar"
			},
			{
				"command": "adabuild.reset",
				"title": "adabuild: Reset Changes"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"vsce": "^2.6.7"
	},
	"dependencies": {
		"@kuroi/syringe": "^2.0.0"
	}
}